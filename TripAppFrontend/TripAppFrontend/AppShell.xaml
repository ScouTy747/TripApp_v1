<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TripAppFrontend.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TripAppFrontend"
    Shell.FlyoutBehavior="Disabled"
    Title="TripAppFrontend">

   

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:Login}"
        Route="LoginPage" />

    <ShellContent
        Title="SignUp"
        ContentTemplate="{DataTemplate local:SignUp}"
        Route="SignUpPage" />

    <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="EditTrip"
        ContentTemplate="{DataTemplate local:EditTrip}"
        Route="EditTripPage" />

    <ShellContent
          Title="UserEdit"
          ContentTemplate="{DataTemplate local:UserEdit}"
          Route="UserEditPage" />

</Shell>
